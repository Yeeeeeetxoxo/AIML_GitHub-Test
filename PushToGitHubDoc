class User:
    Person = User("Ayam", "Ayam@gmail.com", "Perempuan")
    
    def __init__(self, username, email, gender):
      self.user_username  = username
      self.user_email     = email
      self.user_gender    = gender

    print(Person.user_username)
    print(Person.user_email)
    print(Person.user_gender)
